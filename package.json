{
  "name": "cloud-script",
  "version": "1.0.0",
  "description": "",
  "main": "web.js",
  "scripts": {
    "package": "npx esbuild ./src/index.ts --outdir=packaged --format=esm --bundle --minify --target=chrome58",
    "bootstrap": "npm ci && npm run bootstrap:asl-lib && npm run bootstrap:ts-asl",
    "test": "concurrently npm:test:asl-lib npm:test:ts-asl",
    "test:asl-lib": "cd asl-lib && npm run test",
    "bootstrap:asl-lib": "cd asl-lib && npm ci",
    "test:ts-asl": "cd ts-asl && npm run test",
    "bootstrap:ts-asl": "cd ts-asl && npm ci"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.0.0"
  }
}
