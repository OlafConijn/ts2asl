{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "states:StartExecution",
      "Resource": [
        "${Token[CDK.REF.REPLACED]}"
      ]
    },
    {
      "Effect": "Allow",
      "Action": "events:PutRule",
      "Resource": [
        "arn:aws:events:${Token[CDK.REF.REPLACED]}:${Token[CDK.REF.REPLACED]}:rule/StepFunctionsGetEventsForStepFunctionsExecutionRule"
      ]
    },
    {
      "Effect": "Allow",
      "Action": "events:PutTargets",
      "Resource": [
        "arn:aws:events:${Token[CDK.REF.REPLACED]}:${Token[CDK.REF.REPLACED]}:rule/StepFunctionsGetEventsForStepFunctionsExecutionRule"
      ]
    },
    {
      "Effect": "Allow",
      "Action": "events:DescribeRule",
      "Resource": [
        "arn:aws:events:${Token[CDK.REF.REPLACED]}:${Token[CDK.REF.REPLACED]}:rule/StepFunctionsGetEventsForStepFunctionsExecutionRule"
      ]
    }
  ]
}