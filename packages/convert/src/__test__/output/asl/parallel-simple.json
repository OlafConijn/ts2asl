{
  "StartAt": "Initialize",
  "States": {
    "Initialize": {
      "Type": "Pass",
      "ResultPath": "$",
      "Parameters": {
        "vars.$": "$$.Execution.Input",
        "_undefined": null
      },
      "Next": "Parallel"
    },
    "Parallel": {
      "Branches": [
        {
          "StartAt": "worker()",
          "States": {
            "worker()": {
              "Type": "Task",
              "ResultPath": "$.vars.return_var",
              "Resource": "[!lambda[worker]arn]",
              "Retry": [
                {
                  "ErrorEquals": [
                    "Lambda.ServiceException",
                    "Lambda.AWSLambdaException",
                    "Lambda.SdkClientException"
                  ],
                  "IntervalSeconds": 2,
                  "MaxAttempts": 6,
                  "BackoffRate": 2
                }
              ],
              "Comment": "source: worker()",
              "Next": "Return"
            },
            "Return": {
              "Type": "Pass",
              "InputPath": "$.vars.return_var",
              "End": true
            }
          }
        },
        {
          "StartAt": "worker()_1",
          "States": {
            "worker()_1": {
              "Type": "Task",
              "ResultPath": "$.vars.return_var",
              "Resource": "[!lambda[worker]arn]",
              "Retry": [
                {
                  "ErrorEquals": [
                    "Lambda.ServiceException",
                    "Lambda.AWSLambdaException",
                    "Lambda.SdkClientException"
                  ],
                  "IntervalSeconds": 2,
                  "MaxAttempts": 6,
                  "BackoffRate": 2
                }
              ],
              "Comment": "source: worker()",
              "Next": "Return_1"
            },
            "Return_1": {
              "Type": "Pass",
              "InputPath": "$.vars.return_var",
              "End": true
            }
          }
        }
      ],
      "ResultPath": "$.tmp.result",
      "Comment": "source: Promise.all([worker(), worker()])",
      "Type": "Parallel",
      "Next": "Return_2"
    },
    "Return_2": {
      "Type": "Pass",
      "InputPath": "$.tmp.result",
      "End": true
    }
  }
}