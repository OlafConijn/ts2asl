{
  "StartAt": "Initialize",
  "States": {
    "Initialize": {
      "Type": "Pass",
      "ResultPath": "$",
      "Parameters": {
        "vars.$": "$$.Execution.Input",
        "_undefined": null
      },
      "Next": "PutItem"
    },
    "Empty Catch": {
      "Type": "Pass",
      "Next": "PutItem"
    },
    "PutItem": {
      "ResultPath": null,
      "Resource": "arn:aws:states:::aws-sdk:dynamodb:putItem",
      "Parameters": {
        "Item": {
          "pk": {
            "S": "pk-value"
          },
          "sk": {
            "S": "sk-value"
          },
          "string": {
            "S": "value"
          },
          "number": {
            "N": "42"
          }
        },
        "ConditionExpression": "attribute_not_exists(pk)",
        "TableName": "[!parameter[assignmentsTableName]]"
      },
      "Catch": [
        {
          "ErrorEquals": [
            "DynamoDb.ConditionalCheckFailedException"
          ],
          "ResultPath": null,
          "Next": "Empty Catch"
        }
      ],
      "Type": "Task",
      "End": true
    }
  }
}