{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": [
      "es2018",
      "es2019"
    ],
    "allowJs": true,
    "noImplicitAny": true,
    "declaration": true,
    "esModuleInterop": true,
    "noEmit": true,
    "outDir": "./lib",
    "strict": true,
    "resolveJsonModule": true,
  },
  "include": [
    "**/*.ts"
  ],
}