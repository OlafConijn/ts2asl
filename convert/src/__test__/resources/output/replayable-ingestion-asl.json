{
  "StartAt": "Initialize",
  "States": {
    "Initialize": {
      "Type": "Pass",
      "ResultPath": "$",
      "Parameters": {
        "vars.$": "$$.Execution.Input"
      },
      "Next": "replayPrefixer(input)"
    },
    "replayPrefixer(input)": {
      "Type": "Task",
      "ResultPath": "$.vars.result",
      "Resource": "typescript:replayPrefixer",
      "InputPath": "$.vars",
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException"
          ],
          "IntervalSeconds": 2,
          "MaxAttempts": 6,
          "BackoffRate": 2
        }
      ],
      "Comment": "source: replayPrefixer(input)",
      "Next": "Map"
    },
    "Map": {
      "Type": "Map",
      "ResultPath": "$.lastResult",
      "Iterator": {
        "StartAt": "StartExecution",
        "States": {
          "StartExecution": {
            "Type": "Task",
            "ResultPath": "$.vars._var",
            "Resource": "arn:aws:states:::aws-sdk:sfn:startExecution",
            "Parameters": {
              "Input.$": "States.Format('{}', $.vars.prefix)",
              "StateMachineArn": "unresolved parameter: stateMachineArn"
            },
            "Next": "Pass"
          },
          "Pass": {
            "End": true,
            "Type": "Pass",
            "InputPath": "$.vars._var"
          }
        }
      },
      "ItemsPath": "$.vars.result",
      "MaxConcurrency": 5,
      "End": true
    }
  }
}