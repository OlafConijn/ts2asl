{
  "StartAt": "Initialize",
  "States": {
    "Initialize": {
      "Type": "Pass",
      "ResultPath": "$",
      "Parameters": {
        "vars.$": "$$.Execution.Input"
      },
      "Next": "replayPrefixer(input)"
    },
    "replayPrefixer(input)": {
      "Type": "Task",
      "ResultPath": "$.vars.result",
      "Resource": "lambda:replayPrefixer",
      "InputPath": "$.vars",
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException"
          ],
          "IntervalSeconds": 2,
          "MaxAttempts": 6,
          "BackoffRate": 2
        }
      ],
      "Comment": "source: replayPrefixer(input)",
      "Next": "Map"
    },
    "Map": {
      "Type": "Map",
      "ResultPath": "$.tmp.lastResult",
      "Iterator": {
        "StartAt": "Assign Sfn_input",
        "States": {
          "Assign Sfn_input": {
            "Type": "Pass",
            "ResultPath": "$.tmp.sfn_input",
            "Parameters": {
              "prefix.$": "$.vars.prefix",
              "AWS_STEP_FUNCTIONS_STARTED_BY_EXECUTION_ID.$": "$$.Execution.Id"
            },
            "Next": "replayWorker({ prefix })"
          },
          "replayWorker({ prefix })": {
            "Type": "Task",
            "Resource": "arn:aws:states:::aws-sdk:sfn:startExecution",
            "Parameters": {
              "Input.$": "States.JsonToString($.tmp.sfn_input)",
              "StateMachineArn": "statemachine:replayWorker"
            },
            "Retry": [
              {
                "ErrorEquals": [
                  "Lambda.ServiceException",
                  "Lambda.AWSLambdaException",
                  "Lambda.SdkClientException"
                ],
                "IntervalSeconds": 2,
                "MaxAttempts": 6,
                "BackoffRate": 2
              }
            ],
            "Comment": "source: replayWorker({ prefix })",
            "End": true
          }
        }
      },
      "ItemsPath": "$.vars.result",
      "MaxConcurrency": 5,
      "End": true
    }
  }
}