{
  "StartAt": "Initialize",
  "States": {
    "Initialize": {
      "Type": "Pass",
      "ResultPath": "$",
      "Parameters": {
        "vars.$": "$$.Execution.Input",
        "_undefined": null
      },
      "Next": "Eval Conditional"
    },
    "Eval Conditional": {
      "Type": "Choice",
      "Choices": [
        {
          "Not": {
            "Or": [
              {
                "Variable": "$.vars",
                "IsPresent": false
              },
              {
                "Variable": "$.vars",
                "IsNull": true
              }
            ]
          },
          "Next": "Conditional True"
        }
      ],
      "Default": "Conditional False"
    },
    "Conditional True": {
      "Type": "Pass",
      "ResultPath": "$.tmp.var",
      "InputPath": "$.vars.name",
      "Next": "Return args?.name"
    },
    "Conditional False": {
      "Type": "Pass",
      "ResultPath": "$.tmp.var",
      "InputPath": "$._undefined",
      "Next": "Return args?.name"
    },
    "Return args?.name": {
      "Type": "Pass",
      "InputPath": "$.tmp.var",
      "End": true
    }
  }
}