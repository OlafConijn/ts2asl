{
  "StartAt": "Initialize",
  "States": {
    "Initialize": {
      "Type": "Pass",
      "ResultPath": "$",
      "Parameters": {
        "vars.$": "$$.Execution.Input"
      },
      "Next": "Parallel"
    },
    "Parallel": {
      "Branches": [
        {
          "StartAt": "Throw RetryableError",
          "States": {
            "Throw RetryableError": {
              "Type": "Fail",
              "Error": "RetryableError",
              "Cause": "retry me",
              "Comment": "source: throw new RetryableError(\"retry me\")"
            }
          }
        }
      ],
      "ResultPath": "$.tmp.lastResult",
      "Type": "Parallel",
      "Retry": [
        {
          "ErrorEquals": [
            "RetryableError"
          ],
          "IntervalSeconds": 3,
          "MaxAttempts": 2,
          "BackoffRate": 1.5
        }
      ],
      "End": true
    }
  }
}